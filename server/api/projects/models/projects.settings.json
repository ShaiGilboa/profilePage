{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "projects"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "link_to_project": {
      "type": "string"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "project": {
      "type": "component",
      "repeatable": true,
      "component": "project.project"
    }
  }
}
